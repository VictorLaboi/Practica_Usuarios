<Application x:Class="Kanban.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PracticaVentas"
             xmlns:viewModel ="clr-namespace:Kanban.ViewModels"
             xmlns:view="clr-namespace:Kanban.Views"
             >
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModel:KanbanMainViewModel}">
            <view:Form/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModel:LoginViewModel}">
            <view:Login/>
        </DataTemplate>

        <ResourceDictionary x:Key="R">
        </ResourceDictionary>
    </Application.Resources>
</Application>
